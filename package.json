{
    "name": "vactory-nextjs",
    "private": true,
    "version": "0.0.1",
    "main": "index.js",
    "license": "MIT",
    "packageManager": "yarn@1.22.15",
    "engines": {
        "node": ">=16.13.0"
    },
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "test": "jest",
        "dev": "turbo run dev --parallel",
        "dev:starter": "lerna run --parallel --scope starter dev",
        "prepare": "lerna run prepare",
        "watch": "lerna run watch",
        "lint": "eslint --cache --ext .js,.jsx .",
        "phpcs": "./drupal/vendor/bin/phpcs -s -n --colors --standard=modules/next/phpcs.xml.dist modules/next",
        "test:next": "./drupal/vendor/bin/phpunit -c ./drupal/web modules/next",
        "sync:modules": "./scripts/sync-modules.sh \"modules/*\"",
        "sync:examples": "./scripts/sync-examples.sh \"examples/example-*\"",
        "sync:starters": "./scripts/sync-starters.sh \"starters/*\"",
        "seed": "NUM_NODES=200 node ./scripts/seed",
        "cy:open": "turbo run cy:open --parallel",
        "test:e2e": "turbo run test:e2e --parallel",
        "test:e2e:ci": "turbo run test:e2e:ci --parallel"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "jest": {
        "testMatch": [
            "**/packages/**/test/*.{js,ts,tsx}",
            "**/packages/**/*.test.js"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/fixtures/",
            "/.cache/",
            "/dist/"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/eslint-parser": "^7.16.3",
        "@babel/plugin-transform-runtime": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "@babel/preset-react": "^7.9.4",
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^16.2.1",
        "@next/eslint-plugin-next": "^12.0.9",
        "@testing-library/cypress": "^7.0.0",
        "@types/node": "^17.0.21",
        "@types/react": "^17.0.0",
        "@typescript-eslint/eslint-plugin": "^4.0.0",
        "@typescript-eslint/parser": "^3.9.1",
        "autoprefixer": "^10.4.2",
        "cypress": "^9.1.1",
        "eslint": "^7.13.0",
        "eslint-config-next": "^12.0.10",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-cypress": "^2.11.1",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.19.0",
        "faker": "^5.4.0",
        "glob": "^7.2.0",
        "husky": "^4.2.5",
        "jest": "^26.6.3",
        "lerna": "^3.20.2",
        "microbundle": "^0.14.2",
        "path-to-regexp": "^6.2.0",
        "postcss": "^8.4.5",
        "prettier": "^2.0.4",
        "pretty-quick": "^3.1.3",
        "start-server-and-test": "^1.14.0",
        "tailwindcss": "^3.0.15",
        "turbo": "^1.1.1",
        "typescript": "^4.5.4",
        "uuid": "^8.3.2",
        "yaml": "^1.10.2"
    },
    "dependencies": {
        "@headlessui/react": "^0.3.1",
        "@loadable/component": "^5.14.1",
        "@next/bundle-analyzer": "^12.1.0",
        "@react-google-maps/api": "^1.9.0",
        "@tailwindcss/typography": "^0.5.0",
        "@vactory/next": "*",
        "@vactory/next-news": "*",
        "@vactory/next-page": "*",
        "animated-number-react": "^0.1.1",
        "axios": "^0.26.0",
        "classnames": "^2.2.6",
        "clsx": "^1.1.1",
        "cogo-toast": "^4.2.3",
        "copy-to-clipboard": "^3.3.1",
        "cross-env": "^7.0.3",
        "dayjs": "^1.10.4",
        "deepmerge": "^4.2.2",
        "framer-motion": "^3.3.0",
        "fslightbox-react": "^1.4.9",
        "fuse.js": "^6.0.0",
        "html-react-parser": "^0.10.3",
        "http-browserify": "^1.7.0",
        "https-browserify": "^1.0.0",
        "i18next": "^21.6.12",
        "i18next-browser-languagedetector": "^4.1.1",
        "icomoon-react": "^2.0.16",
        "is-client": "^0.0.2",
        "js-cookie": "^2.2.1",
        "jsdom": "^16.4.0",
        "jwt-decode": "^3.1.2",
        "kitsu-core": "^8.1.1",
        "next": "^12.1.0",
        "next-auth": "^4.2.1",
        "next-compose-plugins": "^2.2.1",
        "next-intl": "^2.4.0",
        "next-translate": "^1.3.4",
        "nprogress": "^0.2.0",
        "querystring": "^0.2.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "serve": "^13.0.2",
        "swr": "^1.2.2"
    },
    "resolutions": {}
}
